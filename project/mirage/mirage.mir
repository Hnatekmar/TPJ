(importuj "mirage/listy/listy.mir")
(importuj "mirage/makra/makra.mir")
(importuj "mirage/grafika/grafika.mir")

(definujExpanzi ' nevyhodnocuj)

(defn < (a b)
	(pokud (= a b)
		 NEPRAVDA
         (neguj (> a b))))
 
(defn <= (a b)
	(pokud (= a b)
         PRAVDA
         (neguj (> a b))))

(defn >= (a b)
	(nebo (= a b) (> a b)))
