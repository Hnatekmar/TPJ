; Makra
(definuj infix (makro (x)
		      ; (1 + 2)
		      (definuj op (prvni (zbytek x)))
		      (definuj a (prvni x))
		      (definuj b (prvni (zbytek (zbytek x))))
		      (list op a b)))

(infix (1 + 2))

(definuj dfn (makro (nazev argumenty telo)
		    (list definuj
			  nazev
			  (list funkce argumenty telo))))

(dfn foo (x y ...) (* x y (prvni ...)))

(foo 5 6 7 8 9)
